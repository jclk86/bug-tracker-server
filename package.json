{
  "name": "chimuwaku-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register src/server.ts",
    "dev": "nodemon --config nodemon.json --inspect-brk src/server.ts",
    "build": "tsc",
    "knex:migrate:make": "knex --knexfile src/database/knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.95.4",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
